#!/usr/bin/env sh



# exit if error happens
set -e



# before up
../cli/up-before



# go to docker
script_dir="$( cd "$(dirname "$0")" ; pwd -P )"
cd "$script_dir"
cd "../docker"



# load environment
. ../.env



# compose
docker-compose \
  --env-file ../.env \
  --project-name mojeklima \
  up \
  --detach \
  --build



# after up
../cli/up-after
